    "# Client test\n",
    "\n",
    "import socket\n",
    "import time\n",
    "\n",
    "sock = socket.socket()\n",
    "sock.connect(('localhost', 9090))\n",
    "for i in range(0,100):\n",
    "    sock.send(b'hello, world!')\n",
    "    data = sock.recv(1024)\n",
    "    print(data)\n",
    "    time.sleep(4)\n",
    "    \n",
    "print(\"100 sent\")\n",
    "sock.close()\n",
    "\n",
    "print(data)"
   ]
